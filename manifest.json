{
  "manifest_version": 2,

  "name": "NewWall",
  "description": "Replaces your new tab page with a personal dashboard featuring to-do, weather, inspiration and did you know fact.",
  "version": "2.12.2",

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "permissions": ["geolocation"],

  "background": {
    "scripts": ["background.js", "chrome.js"],
    "persistent": false
  },

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
